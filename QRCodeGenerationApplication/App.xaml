<Application x:Class="QRCodeGenerationApplication.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:QRCodeGenerationApplication"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Assets/GlobalResources.xaml"/>
                <ResourceDictionary Source="/Assets/GlobalStyle.xaml"/>
                <ResourceDictionary Source="/Assets/Icons.xaml"/>
                <ResourceDictionary Source="/Assets/CreateIcon.xaml"/>
                <ResourceDictionary Source="/Assets/UploadIcon.xaml"/>
                <ResourceDictionary Source="/Assets/QRCodeIcon.xaml"/>
                <ResourceDictionary Source="/Assets/RadioButtonTemplateStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
